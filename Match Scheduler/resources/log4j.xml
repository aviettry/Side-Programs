<Configuration status="debug" name="Logger" packages="">
	<Appenders>

		<Appender>
			<Console name="STDOUT" target="SYSTEM_OUT">
				<PatternLayout>
					pattern="%p %m%n"
				</PatternLayout>
			</Console>
		</Appender>

		

	</Appenders>

	<Root level="debug">
		<AppenderRef ref="STDOUT" />
	</Root>
</Configuration>